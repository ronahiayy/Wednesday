{tweet.text}\")\n","            if[ele for ele in ilceler if(ele in tweet.text.split())]:\n","              for item in ilceler:\n","                if item in tweet.text.split():\n","                  alan = item\n","\n","              print(\"\\n\\n\\n --------------------------------------------------------------------------- \\n\")\n","              print(\"Detected tweet from the area: \" + alan +\"\\nTweet =\" +tweet.text)\n","              y_pred=logreg.predict([tweet.text])\n","              print(\"\\n --------------------------------------------------------------------------- \\n\")\n","              print(y_pred + \" Tweet reports over dentisy!\")\n","              if(y_pred[0] is '1' or 1 ):\n","                print(\"\\n --------------------------------------------------------------------------- \\n\")\n","                print(\"Tweeting in progres...\")\n","                api.update_status(\"I Detect over dentisy in area: \" + alan)\n","                print(\"Tweet = I Detect over dentist in area: \" + alan)\n","                print(\"\\n --------------------------------------------------------------------------- \\n\")\n","                print(\"Succesfully tweeted!\")\n","                time.sleep(5)\n","\n","                \n","    def on_error(self, status):\n","        print(\"Error detected\")\n","\n","# Authenticate to Twitter\n","auth = tweepy.OAuthHandler(\"okG5KfqLOW2lQp2LTE8rfbxgk\", \"Jj69hYjBkn36qKvDT1m1dEkQ1jWjRnKCcrbDJGmqSD5VWXkLIQ\")\n","auth.set_access_token(\"784660604-Hn1JdsPheWBmHaD3Aw4SfoxT73F1ZYOPdZRST4rU\", \"2cdtjcWzx91Wqwz8dQj7PWxkiQqq8UQ7r4QZerJD1S41C\")\n","\n","# Create API object\n","api = tweepy.API(auth, wait_on_rate_limit=True,\n","    wait_on_rate_limit_notify=True)\n","\n","tweets_listener = MyStreamListener(api)\n","stream = tweepy.Stream(api.auth, tweets_listener)\n","stream.filter(track=[\"kalabalik\", \"kalabalık\",\"istanbul\"])\n","\n"],"execution_count":null,"outputs":[]}]}